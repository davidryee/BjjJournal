<h1>New Match</h1>
<%= form_for :match, url: matches_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>

    <p>
      <%= f.label :name%><br>
      <%= f.text_area :name%><br>
    </p>

    <p>
      <%= f.label :height%><br>
      <%= f.text_area :height%><br>
    </p>

    <p>
      <%= f.label :weight%><br>
      <%= f.text_area :weight%><br>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>